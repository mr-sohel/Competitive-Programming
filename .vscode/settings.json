{
    "editor.fontFamily": "'Monaco','Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.fontSize": 15,
    "terminal.integrated.fontFamily": "monospace",
    "terminal.integrated.fontSize": 15,
    "editor.autoIndent": "full",
    "editor.formatOnPaste": true,
    "editor.formatOnType": true,
    "files.associations": {
        "iostream": "cpp",
        "bitset": "cpp",
        "random": "cpp",
        "ostream": "cpp",
        "string": "cpp",
        "cmath": "cpp",
        "array": "cpp",
